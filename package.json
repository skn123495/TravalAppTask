    "react-native-svg": "^13.9.0",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-vector-icons": "^10.0.3",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^8.0.5",
    "react-test-renderer": "18.1.0",
