{
  "name": "travelapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios --simulator='iPhone 14'",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reset": "npx react-native start --reset-cache",
    "tcp": "adb reverse tcp:8081 tcp:8081",
    "pod": "cd ios && pod install",
    "pod-clean": "cd ios && rm -rf Pods && rm -rf Podfile.lock &&  pod install --repo-update && cd ..",
    "pod-cache-clean": "rm -rf ~/Library/Caches/CocoaPods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && pod cache clean --all && cd ..",
    "release": "cd android && ./gradlew assembleRelease && cd .. && run-s -c open-android-folder open-diawi",
    "bundle": "cd android && ./gradlew bundleRelease && cd .. && run-s -c open-bundle-folder open-play-console",
    "clean": "cd android && ./gradlew clean && cd ..",
    "apk-p": "run-s -c kill-all env-p clean release",
    "apk-d": "run-s -c kill-all env-d clean release",
    "apk-b": "run-s -c kill-all env-b clean release",
    "bundle-s": "run-s -c kill-all env-d clean bundle",
    "bundle-p": "run-s -c kill-all env-p clean bundle",
    "bundle-b": "run-s -c kill-all env-b clean bundle"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-community/netinfo": "^9.4.1",
